{
  "name": "sistema-loja-roupas",
  "version": "1.0.0",
  "description": "Sistema de gerenciamento para pequena loja de roupas",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "seed": "node server/utils/seed.js",
    "install-all": "npm install && npm install --prefix client"
  },
  "dependencies": {
    "axios": "^1.8.1",
    "bcrypt": "^5.1.1",
    "concurrently": "^7.6.0",
    "csv-parser": "^3.2.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.9",
    "react-input-mask": "^2.0.4",
    "sqlite3": "^5.1.7",
    "xlsx": "^0.18.5"
  }
}
